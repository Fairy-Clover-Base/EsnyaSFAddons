			float2 uv_MainTex3 = i.uv_texcoord;
			float4 temp_output_2_0 = ( tex2D( _MainTex, uv_MainTex3 ) * i.vertexColor * _Color );
			o.Albedo = temp_output_2_0.rgb;
			o.Transmission = ( (temp_output_2_0).rgb * _Transmission );
			o.Alpha = (temp_output_2_0).a;
		}

		ENDCG
		CGPROGRAM
		#pragma surface surf StandardCustom alpha:fade keepalpha fullforwardshadows exclude_path:deferred 

		ENDCG
		Pass
